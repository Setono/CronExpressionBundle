{
    "name": "setono/cron-expression-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle that integrates dragonmantank/cron-expression",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.1",
        "brick/reflection": "~0.2.1",
        "doctrine/doctrine-bundle": "^1.9",
        "dragonmantank/cron-expression": "^2.2",
        "phpdocumentor/reflection-docblock": "^4.3",
        "symfony/config": "^3.4|^4.1",
        "symfony/dependency-injection": "^3.4|^4.1",
        "symfony/form": "^3.4|^4.1"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "phpstan/phpstan": "^0.12.9",
        "phpunit/phpunit": "^7",
        "symplify/easy-coding-standard": "^5.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\CronExpressionBundle\\": ""
        },
        "exclude-from-classmap": [ "/Tests/" ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "check-style": "./vendor/bin/ecs check DependencyInjection/ Doctrine/ Form/ Tests/",
        "fix-style": "./vendor/bin/ecs check --fix DependencyInjection/ Doctrine/ Form/ Tests/",
        "analyse": "./vendor/bin/phpstan analyse -c phpstan.neon -l max"
    }
}
